{
  "name": "inventory-api",
  "version": "1.0.0",
  "description": "A well-structured inventory management API with clean architecture",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "inventory",
    "api",
    "express",
    "typescript",
    "clean-architecture",
    "prisma",
    "mysql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "@prisma/engines": "^6.16.3",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/connect-timeout": "^1.9.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prisma": "^5.15.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "tsx": "^4.20.6",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
